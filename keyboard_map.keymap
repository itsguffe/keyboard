
# parsed and then tweaked from https://github.com/LOWPROKB/zmk-config-zen-2/blob/main/config/corneish_zen.keymap
layout:
  qmk_keyboard: corne_rotated
  qmk_layout: LAYOUT_split_3x6_3
layers:
  QWERTY:
    - ["Tab", "Q", "W", "E", "R", "T", "Y", "U", "I", "O", "P", "/"]
    - ["LShft", "A", "S", "D", "F", "G", "H", "J", "K", "L", ";", "'"]
    - ["LCtrl", "Z", "X", "C", "V", "B", "N", "M", ",", ".", "Bspace", "Esc"]
    - ["LGui", "Number", "Space", "Enter", "Symbol", "RAlt"]
  DANISH:
    - ["Tab", "Q", "W", "E", "R", "T", "Y", "U", "I", "O", "P", "Å"]
    - ["LShft", "A", "S", "D", "F", "G", "H", "J", "K", "L", "Æ", "Ø"]
    - ["LCtrl", "Z", "X", "C", "V", "B", "N", "M", ",", ".", "Bspace", "Esc"]
    - ["LGui", "Number", "Space", "Enter", "Symbol", "RAlt"]
  NUMBER:
    - ["Tab", "1", "2", "3", "4", "5", "6", "7", "8", "9", "0", "Bspace"]
    - ["LShft", "BT 1", "BT 2", "BT 3", "BT 4", "BT 5", "Left", "Down", "Up", "Right", null, null]
    - ["F1", "F2", "F3", "F4", "F5", "F6", "F7", "F8", "F9", "F10", "F11", "F12"]
    - ["LGui", {type: held}, "Space", "Enter", null, "RAlt"]
  SYMBOL:
    - ["Tab", "!", "@", "#", "$", "%", "^", "&", "*", "(", ")", "Bspace"]
    - ["LCtrl", null, null, null, null, null, "-", "=", "[", "]", "\\", "`"]
    - ["LShft", null, null, null, null, null, "_", "+", "{", "}", "|", "~"]
    - ["LGui", null, "Space", "Enter", {type: held}, "RAlt"]
  
